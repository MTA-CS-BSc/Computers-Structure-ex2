/**
 * Or gate: out = 1 if {a==1 or b==1}, 0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=not_a);
    Nand(a=b, b=b, out=not_b);
    Nand(a=not_a, b=not_b, out=out);
}
